<Application x:Class="Master.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Master"
             xmlns:vm="clr-namespace:Master.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <vm:ViewModelLocator x:Key="Locator"></vm:ViewModelLocator>
                </ResourceDictionary>
                <ResourceDictionary Source="/Themes/DefaultColor.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Themes/Zh_CN.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/BrushesStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TextBlockStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/RadioButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Dicgo.Controls;component/Styles/ButtonStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
